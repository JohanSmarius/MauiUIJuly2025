<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAIJuly.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAIJuly"
    Title="MauiAIJuly">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Overview"
        Icon="{StaticResource IconProjects}"
        ContentTemplate="{DataTemplate local:ShiftOverviewPage}"
        Route="projects" />

</Shell>
